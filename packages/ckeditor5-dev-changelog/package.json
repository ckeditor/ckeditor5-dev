  },
  "dependencies": {
    "@ckeditor/ckeditor5-dev-utils": "workspace:*",
    "date-fns": "^4.0.0",
    "glob": "^13.0.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^12.5.2",
    "semver": "^7.6.3",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@types/semver": "^7.7.0",
    "@vitest/coverage-v8": "^4.0.18",
