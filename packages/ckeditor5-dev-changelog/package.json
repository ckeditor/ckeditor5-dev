  "bugs": "https://github.com/ckeditor/ckeditor5/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ckeditor/ckeditor5-dev.git",
    "directory": "packages/ckeditor5-dev-changelog"
  },
  "engines": {
    "node": ">=24.11.0",
    "npm": ">=5.7.1"
  },
  "type": "module",
  "main": "dist/index.js",
